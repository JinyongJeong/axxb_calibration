function F = myfun1(x,e1,e2,Sigma1,Sigma2)


e1 = [0.9981 -0.0037 -0.0610 0.1259; 0.0037 1 -0.0001 0.0615; 0.0610 -0.0002 0.9981 0.0039; 0 0 0 1];
e2 = [0.9981 0.0037 0.0610 -0.1262; -0.0037 1 -0.0002 -0.0609; -0.0610 0 0.9981 0.0037; 0 0 0 1];

Sigma1 = [0.1336    0.0179   -0.0429   -0.0395    0.0674   -0.0950; 0.0179    0.1591   -0.0117   -0.0803    0.0100    0.0137; -0.0429   -0.0117    0.1573    0.1207   -0.0458    0.0295; -0.0395   -0.0803    0.1207    0.3248   -0.0381    0.0155; 0.0674    0.0100   -0.0458   -0.0381    0.2381   -0.0478; -0.0950    0.0137    0.0295    0.0155   -0.0478    0.2720



ad1 = little_adjoint(logm(e1),2);
ad2 = little_adjoint(logm(e2),2);

e12 = expm(hat2(x(37:42)));

ad12 = little_adjoint(logm(e12),2);



count = 0;
for ii = 1 : 6
    for jj = 1 : 6
        count = count + 1;
        Sigma12(ii,jj) = x(count);
    end
end

M1 = inv(Sigma1) - inv(Sigma1)*ad1;
M2 = inv(Sigma2) - inv(Sigma2)*ad2;
M12 = inv(Sigma12) - inv(Sigma12)*ad12;





F = zeros(12,6);
F(1:6) = Sigma12*vee(logm(e12))' - inv(Sigma1)*vee(logm(e1))' - inv(Sigma2)*vee(logm(e2))';
F(7:end,1:6) = M12 + M12' - M1 - M1' - M2 - M2';


